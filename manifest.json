{
  "manifest_version": 2,
  "name": "SFL Prices P2P",
  "version": "1.1",
  "description": "Shows prices for SFL World (P2P) items",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "https://sfl.world/api/v1/prices"
  ],
  "icons": {
    "48": "images/icon.png"
  }
}